    "typescript": "^4.8.4",
    "whatwg-fetch": "^3.6.2"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "resolutions": {
    "inquirer": "^8.0.0"
  },
  "author": "Paalamugan (https://github.com/paalamugan)",
  "msw": {
    "workerDirectory": "public"
  },
