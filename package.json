    "next-connect": "^0.13.0",
    "next-seo": "^5.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "sass": "^1.55.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
